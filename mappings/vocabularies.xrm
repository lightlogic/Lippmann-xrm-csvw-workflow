		E14_Condition_Assessment
        E24_Physical_Human_Made_Thing "E24_Physical_Human-Made_Thing"
        E35_Title
        E36_Visual_Item
        E42_Identifier
        E54_Dimension
        E55_Type
    
    properties
        P1_is_identified_by
        P2_has_type
        P102_has_title
        P34_concerned
        P35_has_identified
        P43_has_dimension
        P45_consists_of
        P46_is_composed_of
        P55_has_current_location
