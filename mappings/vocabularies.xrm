vocabulary schema {
    prefix "schema" "https://schema.org"

    classes
        Name
        Organisation
        Person
        PostalAddress
        QuantitativeValue

    properties
        addressLocality
        addressCountry
        contactPoint
        employee
        hasOccupation
        location
        name
        description
        postalCode
        streetAddress
        unitCode
        value
        depth
        height
        width
}

